{
    "name": "rawphp/raw-base",
    "type": "library",
    "description": "RawBase provides base components for RawPHP Framework and other applications.",
    "keywords": [ "base" ],
    "homepage": "http://rawphp.org",
    "license": "MIT",
    "authors": [
        {
            "name": "Tom Kaczocha",
            "email": "tom@rawphp.org"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "4.2.5",
        "squizlabs/php_codesniffer": "dev-master",
        "rawphp/raw-code-standards": "1.*@dev",
        "phpdocumentor/phpdocumentor": "dev-master"
    },
    "autoload": {
        "classmap": [
            "lib/"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.x-dev"
        }
    }
}
